@model String[]
@{
    Layout = "/Views/Shared/_Layout.cshtml";
}


<div class="container col-md-6">
    <div id="carouselExampleIndicators" class="carousel slide centered-div" data-ride="carousel">

        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="~/Images/SliderItems/HomeSlider/e-ticaret-893x471.png" alt="First slide" />
            </div>
            <div class="carousel-item">
                <img src="~/Images/SliderItems/HomeSlider/e-ticaret-893x536.png" class="d-block w-100" alt="Second slide" />
            </div>
            <div class="carousel-item">
                <img src="~/Images/SliderItems/HomeSlider/web-sitesi-893x472.png" class="d-block w-100" alt="Third slide" />

            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>

    </div>
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
    </ol>
</div>


<section class="py-5 centered-div bg-container">
    <div class="overlay ">
        <div class="container" >
            <div id="carouselExample" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-inner">
                    <div class="row justify-content-center">
                        <div>
                            <div class="row gy-5">
                                <div class="col-md-3 col-sm-6">
                                    <div class="card" >
                                        <img src="~/Images/HomePage/CardImages/ucretsiz-e-ticaret-sitesi-kurmak-491x324.jpg" class="card-img-top" />
                                        <div class="card-body">
                                            <h5 class="card-title">E-Ticaret</h5>
                                            <p class="card-text">
                                                Anahtar teslim
                                                e-ticaret sitesi
                                                Alan adı, hosting, e-mail, güvenlik, e-ticaret paketlerimizde size..
                                            </p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6 ">
                                    <div class="card" >
                                        <img src="~/Images/HomePage/CardImages/karel-arge-tasarim-512x335.jpg" class="card-img-top" />
                                        <div class="card-body">
                                            <h5 class="card-title">Elektronik Ar-GE Yazılım</h5>
                                            <p class="card-text">Profesyonel ekibimizle siz değerli müşterilerimizden gelen talep doğrultusunda elektronik projeler ve yazılımları geliştirmek...</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6 ">
                                    <div class="card" >
                                        <img src="~/Images/HomePage/CardImages/mbr-491x317.jpg" class="card-img-top" />
                                        <div class="card-body">
                                            <h5 class="card-title">Enerji/Otomasyon Sistemleri </h5>
                                            <p class="card-text">Yüksek gerilimden, alçak gerilime kadar anahtar teslim marka bağımsız enerji ve otomasyon çözümleri ile TMK MÜHENDİSLİK yanınızda...</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3 col-sm-6 ">
                                    <div class="card" >
                                        <img src="~/Images/HomePage/CardImages/indir-1-287x175.png" class="card-img-top" />
                                        <div class="card-body">
                                            <h5 class="card-title">
                                                Mobil,Masaüstü ve Web
                                                Proje geliştirme
                                            </h5>
                                            <p class="card-text">
                                                Profesyonel kadromuz ile sizlerin isteği doğrultusunda mobil, masaüstü ve
                                                web projeleri gerçekleştiriyoruz...
                                            </p>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    
</section>









